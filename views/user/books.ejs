            <td><%= books[i].user_id %></td>
            <td><%= books[i].user_name %></td>
            <td>
                <a href="http://localhost:5500/user/<%= books[i].user_id %>">View</a>
                <!-- <a href="http://localhost:5500/user/update/<%= books[i].user_id %>">Edit</a> -->
                <!-- <a href="http://localhost:5500/user/delete/<%= books[i].user_id %>">Delete</a> -->
            </td>
        </tr>
        <% } %>
    </table>
    <div class="link-center">
    <a href="http://localhost:5500/users/create">Create New User</a>
    <a href="http://localhost:5500/">Back to List</a>
    </div>
</body>
</html>